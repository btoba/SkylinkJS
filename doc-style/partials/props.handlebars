<div id="property_{{name}}" class="panel panel-code">
  <div class="panel-heading">
    <h5>
      {{name}}

      {{#if since}}
        <span class="badge">Version {{since}}</span>
      {{/if}}
    </h5>
    {{#if overwritten_from}}
    <p>
      Inherited from
      <a href="{{crossLinkRaw overwritten_from/class}}#method_{{overwritten_from/name}}">{{overwritten_from/class}}</a>
      {{#if foundAt}} but overwritten in {{/if}}
    </p>
    {{else}} {{#if extended_from}}
    <p>
      Inherited from:
      <a href="{{crossLinkRaw extended_from}}#method_{{name}}">{{extended_from}}</a>
    </p>
    {{else}} {{#providedBy}}
    <p>Provided by the <a href="../modules/{{.}}.html">{{.}}</a> module.</p>
    {{/providedBy}} {{/if}} {{/if}} {{#if providedBy}} {{#providedBy}}
    <p>Provided by the <a href="../modules/{{.}}.html">{{.}}</a> module.</p>
    {{/providedBy}} {{/if}} {{#if foundAt}}
    <p>
      Defined in: <a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
    </p>
    {{/if}}
  </div>

  <div class="panel-body">
    <blockquote>
      {{#if default}}
        <div class="code-item-section">
          <h4>Default value:</h4>
          <code class="prettyprint">{{default}}</code>
        </div>
      {{/if}}
  
      {{#if deprecated}}
      <p>
        <em class="glyphicon glyphicon-alert" aria-hidden="true"></em> This method is deprecated. Please refrain from using this as this would be unsupported in future releases. {{#if deprecationMessage}}
        <br>
        <br><small>{{deprecationMessage}}</small>
        {{/if}}
      </p>
      {{/if}} {{#if access}}
      <p>
        <em class="glyphicon glyphicon-eye-close" aria-hidden="true"></em> Access level: {{{access}}}
      </p>
      {{/if}} {{#if final}}
      <p>
        <em class="glyphicon glyphicon-lock" aria-hidden="true"></em> This method cannot be overwritten.
      </p>
      {{/if}} {{#if multiple}}
      <span class="label label-warning" title="This argument may occur one or more times.">multiple</span>
      {{/if}} {{#if required}}
      <span class="label label-warning" title="This attribute is required for communication.">required</span>
      {{/if}} {{#if overwritten_from}}
      <p>
        <em class="glyphicon glyphicon-edit" aria-hidden="true"></em> This method overwrites inherited class method.
      </p>
      {{/if}} {{#if static}}
      <p>
        <em class="glyphicon glyphicon-arrow-left" aria-hidden="true"></em> This method is a static method
      </p>
      {{/if}} {{#if chainable}}
      <p>
        <em class="glyphicon glyphicon-refresh" aria-hidden="true"></em> This method is chainable.
      </p>
      {{/if}} {{#if readonly}}
      <span class="label label-info" title="This attribute is reference only.">readonly</span>
      {{/if}} {{#if async}}
      <p>
        <em class="glyphicon glyphicon-random" aria-hidden="true"></em> This method invokes asycronously.
      </p>
      {{/if}}
    </blockquote>
    <p>{{{propertyDescription}}}</p>
  </div>
  <div class="panel-body">
    <div class="col-md-6">
      {{#if params}}
      <div class="code-section-item">
        <h6>Event Payload:</h6>
        <ul class="params-list">
          {{#params}} {{>param}} {{/params}}
        </ul>
      </div>
      {{/if}}
    </div>


    <div class="col-md-6">
      <pre class="prettyprint">
      // Example for {{name}}
      {{#example}}
        {{{.}}}
      {{/example}}
    </pre>
    </div>
  </div>
</div>
